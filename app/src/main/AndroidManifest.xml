<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ymj.dict">
    <!-- 网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.ymj.dict.UniteApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.ymj.dict.CollectionActivity"></activity>
        <activity android:name="com.ymj.dict.SettingActivity" />
        <activity android:name="com.ymj.dict.ChengyuInfoActivity" />
        <activity android:name="com.ymj.dict.SearchChengyuActivity" />
        <activity android:name="com.ymj.dict.IdentifyImgActivity" />
        <activity android:name="com.ymj.dict.WordInfoActivity" />
        <activity android:name="com.ymj.dict.SearchPinyinActivity" />
        <activity android:name="com.ymj.dict.SearchBuShouActivity" />
        <activity android:name="com.ymj.dict.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>